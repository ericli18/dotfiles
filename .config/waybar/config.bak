{
    "layer": "top",
    "height": 25,
    "modules-left": ["clock", "custom/media"],
    "modules-center": ["sway/workspaces", "sway/mode"],
    "modules-right": ["idle_inhibitor", "tray", "pulseaudio", "network", "battery", "custom/power"],
    "sway/workspaces": {
        "disable-scroll": true
        // "format": "{icon}",
        // "format-icons": {
        //     "1": "1 Ôâ©",
        //     "2": "2 ÔÑ†",
        //     "3": "3 ÔÅµ",
        //     "4": "4 ÔÜº",
        //     "5": "5 ÔÑâ",
        //     "6": "6 ÔÉ†",
        //     "7": "7",
        //     "8": "8",
        //     "9": "9",
        //     "10": "10"
        // }
    },
    "custom/media": {
        "format": "<span color='#b8bb26'>{icon}</span> {}",
        "return-type": "json",
        "max-length": 40,
        "format-icons": {
            "spotify": "ÔÜº",
            "default": "üéú"
        },
        "escape": true,
        // "exec": "$HOME/.config/waybar/mediaplayer.py 2> /dev/null" // Script in resources >
        "exec": "$HOME/.config/waybar/mediaplayer.py", // Filte>
	    "on-click": "playerctl -p spotify play-pause",
        "on-click-right-release": "playerctl -p spotify next",
	    "tooltip": false
    },
    "sway/mode": {
        "format": "<span style=\"italic\">{}</span>"
    },
    "sway/window": {
        "max-length": 50
    },
    "clock": {
        "format": "ÔÄó {:%I:%M %p ÔÅ≥ %e %b}",
	"tooltip": false,
        "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
        "today-format": "<b>{}</b>",
        "on-click": "swaync-client -t"
    },
	"clock#time": {
		"interval": 10,
		"format": "{:%I:%M %p}",
		"tooltip": false
	},
	"clock#date": {
		"interval": 20,
		"format": "{:%e %b %Y}",
		"tooltip": false
	},

     "idle_inhibitor": {
        "format": "{icon}",
        "format-icons": {
            "activated": "ÔÅÆ",
            "deactivated": "ÔÅ∞"
        },
        "tooltip": "true"
    },
    "tray":
    {
        "icon-size": 18,
	"spacing": 10
    },
    "backlight": {
        "format": "{percent}% {icon}",
        "format-icons": ["ÔÑë", "ÔÜÖ"]
    },
    "battery": {
        "bat": "BAT0",
		"interval": 10,
        "states": {
            "warning": 30,
            "critical": 15,
            "5": 100,
            "4": 75,
            "3": 50,
            "2": 30,
            "1": 15
        },
        "format-time": "{H}:{M:02}",
        "format": "{capacity}% {icon}",
        "format-charging-5": "{capacity}% ÔâÄ  Û∞ö•",
        "format-charging-4": "{capacity}% ÔâÅ  Û∞ö•",
        "format-charging-3": "{capacity}% ÔâÇ  Û∞ö•",
        "format-charging-2": "{capacity}% ÔâÉ  Û∞ö•",
        "format-charging-1": "{capacity}% ÔâÑ  Û∞ö•",
		"format-charging-full": "{capacity}%",
        "format-charging": "{capacity}% Û∞ö•",
        "format-alt": "{time} {icon}",
        "format-full": "100% ÔâÄ",
        "format-icons": {
            "1": "ÔâÑ", 
            "2": "ÔâÉ", 
            "3": "ÔâÇ", 
            "4": "ÔâÅ", 
            "5": "ÔâÄ"
        }
    
    },
    "network": {
    	"format": "{ifname}",
	    "format-ethernet": "{ifname} ÔÉÅ",
        "format-wifi": "{essid} Ôá´",
        "format-linked": "{ifname} (No IP) ÔÉÅ",
       	"format-disconnected": "Disconnected ‚ö†",
        "format-alt": "{ifname}: {ipaddr}/{cidr}",
        "tooltip-format": "{ifname}: {ipaddr}/{cidr}"
    },

	"pulseaudio": {
		"format": "{icon} {volume}%",
		"format-bluetooth": "{icon}Ôäî {volume}%",
		"format-muted": "ÔÄ¶",
		"format-icons": {
			"headphone": "ÔÄ•",
			"hands-free": "Ôñê",
			"headset": "Ôñê",
			"phone": "ÔÇï",
			"portable": "ÔÇï",
			"car": "ÔÜπ",
            "default": ["ÔÄ¶", "ÔÄß", "ÔÄ®"]
		},
		"scroll-step": 1,
        "reverse-scrolling": true,
		"on-click": "pactl set-sink-mute @DEFAULT_SINK@ toggle",
		"tooltip": false
	},

    "custom/power": {
        "format": "Û∞ê• ",
        "interval": 86400,
        "on-click": "~/.config/sway/scripts/wlogout.sh",
        "tooltip": true
    }

}
