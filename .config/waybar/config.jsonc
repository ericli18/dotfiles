{
  "layer": "top",
  "height": 25,
  "modules-left": ["tray", "custom/media"],
  "modules-center": ["sway/workspaces", "sway/mode"],
  "modules-right": [
    "idle_inhibitor",
    "pulseaudio",
    "network",
    "battery",
    "clock",
  ],
  "sway/workspaces": {
    "disable-scroll": true,
  },
  "custom/media": {
    "format": "<span color='#b8bb26'>{icon}</span> {}",
    "return-type": "json",
    "max-length": 40,
    "format-icons": {
      "spotify": "ÔÜº",
      "default": "üéú",
    },
    "escape": true,
    "exec": "$HOME/.config/waybar/mediaplayer.py", // Filte>
    "on-click": "playerctl -p spotify play-pause",
    "on-click-right-release": "playerctl -p spotify next",
    "tooltip": false,
  },
  "sway/mode": {
    "format": "<span style=\"italic\">{}</span>",
  },
  "sway/window": {
    "max-length": 50,
  },
  "mpris": {
    "format": "{player_icon} {dynamic}",
    "format-paused": "{status_icon} <i>{dynamic}</i>",
    "player-icons": {
      "spotify": "ÔÜº",
      "default": "üéµ",
    },
    "dynamic-order": ["title", "artist"],
    "interval": 5,
    "status-icons": {
      "paused": "‚è∏",
    },
    "ignored-players": ["firefox"],
  },
  "clock": {
    "format": "{:%a %b %d  %I:%M %p}",
    "tooltip": false,
    "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
    "today-format": "<b>{}</b>",
    "on-click": "swaync-client -t",
  },
  "clock#time": {
    "interval": 10,
    "format": "{:%I:%M %p}",
    "tooltip": false,
  },
  "clock#date": {
    "interval": 20,
    "format": "{:%e %b %Y}",
    "tooltip": false,
  },
  "idle_inhibitor": {
    "format": "{icon}",
    "format-icons": {
      "activated": "ÔÅÆ",
      "deactivated": "ÔÅ∞",
    },
    "tooltip": "true",
  },
  "tray": {
    "icon-size": 18,
    "spacing": 10,
  },
  "backlight": {
    "format": "{percent}% {icon}",
    "format-icons": ["ÔÑë", "ÔÜÖ"],
  },
